{
  "name": "knex-automigrate",
  "version": "0.0.6",
  "description": "Table schema based database migration tool, built on top of the knex.js",
  "main": "index.js",
  "scripts": {
    "test": "standard && mocha --reporter spec --bail --check-leaks test/index.js",
    "lint": "standard",
    "lintfix": "standard --fix",
    "lintmon": "./node_modules/nodemon/bin/nodemon.js --exec './node_modules/standard/bin/cmd.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/why2pac/knex-automigrate.git"
  },
  "keywords": [
    "nodejs",
    "knexjs",
    "database",
    "orm",
    "table",
    "migration"
  ],
  "dependencies": {
    "knex": "^0.14.6",
    "commander": "^2.15.1",
    "chalk": "^2.4.1",
    "tildify": "^1.2.0",
    "liftoff": "^2.5.0",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "mocha": "^5.2.0",
    "mysql": "^2.15.0",
    "nodemon": "^1.17.5",
    "standard": "^11.0.1"
  },
  "bin": {
    "knex-automigrate": "./bin/cli.js"
  },
  "files": [
    "README.md",
    "bin/*",
    "lib/*",
    "index.js",
    "LICENSE"
  ],
  "standard": {
    "env": [
      "mocha"
    ]
  },
  "author": "GONZO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/why2pac/knex-automigrate/issues"
  },
  "homepage": "https://github.com/why2pac/knex-automigrate#readme"
}
